      <div class="close_sidebar padding_5 cursor_pointer">
        <i class="fas fa-chevron-double-left color_white"></i>
      </div>
    </div>
    <!-- search  -->
    <div class="search margin_top_20 position_relative padding_x_10">
      <input
        class="padding_right_20 padding_left_35 padding_y_8 bg_whitesmoke border_none width_100_percent border_radius_5 color_white"
        placeholder="Search"
      />
      <i class="fa fa-search position_absolute left_20 top_9"></i>
    </div>
    <!-- navigation  -->
    <div
      class="navigation margin_top_10 border_width_0 border_bottom_width_1 border_style_solid border_color_primary padding_bottom_15"
    >
      <div
        class="display_flex padding_y_10 align_items_center padding_x_20 cursor_pointer nav_item_side_bar active_content_main"
      >
        <div class="icon margin_right_10 width_20">
          <i class="fas fa-home"></i>
        </div>
        <div class="title">
          <span class="text_transform_capitalize">Home</span>
        </div>
      </div>
      <a class="display_flex padding_y_10 align_items_center padding_x_20 cursor_pointer nav_item_side_bar" href="/notifications">
        <div class="icon margin_right_10 width_20">
          <i class="fas fa-bell"></i>
        </div>
        <div class="title">
          <span class="text_transform_capitalize">Notification</span>
        </div>
      </a>
    </div>
    <!-- space sprint  -->
    <%- include("space/space", { spaceList: spaceList } );%>
  </div>
  <!-- side bar settings user  -->
  <div class="position_relative wrapper_settings no_select">
    <%- include('common-settings/common-settings', { workspace: workspace, workspaceDenote: workspaceDenote }); %>
    <div
      class="settings position_relative display_flex align_items_center padding_left_40 padding_top_20 border_width_0 border_top_width_1 border_style_solid border_color_primary"
    >
      <div
        id="user_avatar"
        class="user_avatar cursor_pointer font_weight_700 position_relative border_radius_50_percent width_40 height_40 bg_highlight display_flex align_items_center justify_content_center border_width_2 border_color_white border_style_solid"
      >
        <span class="user_name">C</span>
        <div
          id="workspace_avatar"
          style="left: -30px; top: 0px; background-color: <%=workspace.color%>"
          class="workspace_avatar position_absolute font_weight_700 border_radius_50_percent width_40 height_40 display_flex align_items_center justify_content_center border_width_2 border_color_white border_style_solid"
        >
          <%=workspaceDenote%>
        </div>
      </div>
      <i class="cursor_pointer fas fa-sort-down margin_left_5"></i>
    </div>
  </div>
</div>
